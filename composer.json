{
  "name": "vitorsreis/analysis",
  "description": "Monitor/Profiler for PHP",
  "version": "2.0.0",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/vitorsreis/analysis",
  "authors": [
    {
      "name": "Vitor Reis",
      "email": "vitor@d5w.com.br"
    }
  ],
  "keywords": [
    "performance",
    "analyzer",
    "profile",
    "requests",
    "server",
    "monitor",
    "debug",
    "php"
  ],
  "autoload": {
    "psr-4": {
      "VSR\\Analysis\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.13",
    "vitorsreis/phpunit-dispatcher": "1.0.0"
  },
  "suggest": {
    "ext-pdo": "For use SQLite database storage",
    "ext-zlib": "For use gzip compression"
  }
}